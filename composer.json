{
  "name": "arturmamedov/xliff-translation-workflow",
  "description": "XLIFF Brand Voice Translation Automation for WPML workflow",
  "type": "library",
  "version": "1.0.0",
  "authors": [
    {
      "name": "Artur Mamedov",
      "email": "arturmamedov1993@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "NestsHostels\\XLIFFTranslation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NestsHostels\\XLIFFTranslation\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "translate": "php bin/translate.php"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
